<?xml version="1.0"?>

<PropertyList>
    <generic>
        <output>
            <line_separator>newline</line_separator>
            <var_separator>,</var_separator>

            <!-- Position -->
            <chunk>
				<name>altitude</name>
				<node>/position/altitude-ft</node>
				<type>float</type>
				<format>%.2f</format>
			</chunk>
			
            <chunk>
                <name>latitude (decimal)</name>
                <node>/position/latitude-deg</node>
                <type>double</type>
                <format>%.8f</format>
            </chunk>
            
             <chunk>
                <name>longitude (decimal)</name>
                <node>/position/longitude-deg</node>
                <type>double</type>
                <format>%0.8f</format>
            </chunk>
        </output>
		
		<input>
			<line_separator>newline</line_separator>
			<var_separator>,</var_separator>
			<chunk>
                <name>latitude (decimal)</name>
                <node>/environment/aloftwind/latitude-deg</node>
                <type>double</type>
                <format>%.8f</format>
            </chunk>
            
             <chunk>
                <name>longitude (decimal)</name>
                <node>/environment/aloftwind/longitude-deg</node>
                <type>double</type>
                <format>%0.8f</format>
            </chunk>
